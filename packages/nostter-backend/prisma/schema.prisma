// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url = env("DATABASE_URL")
}

model Relay {
	url String @id
}

model Event {
	id String @id
	sig String
	pubkey String
	kind Int
	tags Json
	content String
	createdAt DateTime
}
